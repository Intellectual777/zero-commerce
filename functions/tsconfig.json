{
	"compilerOptions": {
		"target": "ESNext",
		"lib": ["ESNext"],
		"module": "ESNext",
		"moduleResolution": "Node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"skipLibCheck": true,
		"declaration": true,
		"composite": true,
		"types": ["@cloudflare/workers-types"]
	}
}
